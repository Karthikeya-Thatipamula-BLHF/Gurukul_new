# Frontend production environment example
# Copy as .env.production and set to your production endpoints

# Backend API URLs
VITE_API_BASE_URL=https://api.yourdomain.com
VITE_CHAT_API_BASE_URL=https://chat.yourdomain.com
VITE_FINANCIAL_API_BASE_URL=https://finance.yourdomain.com
VITE_AGENT_API_BASE_URL=https://agent.yourdomain.com
VITE_UNIGURU_API_BASE_URL=https://uniguru.yourdomain.com

# Feature flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Local/dev proxy debugging (keep false in prod)
VITE_DEBUG_PROXY=false

# Optional API keys used by dev proxy only (not needed in prod build)
VITE_VISION_API_KEY=

